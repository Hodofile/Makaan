        <div class="panel panel-default">
          <div class="panel-heading"><h1>Project associated with <%= sdata.ad[0].name%></h1></div>
          <div class="panel-body">
            

            <div id="port-folio">
              <div class="row">
                
                <% for(let k of sdata.ap) { %>
                <div class="col-md-4">
                <div class="thumbnail">
                    <img src="/images/<%= k.image %>" class="img-responsive" />
                    <div class="caption">
                <a href="/single/<%=k._id%>" <h1 class="text-center"><%=k.pname%></h1></a>
                </div>
                </div>
                </div>
                <% } %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- /. PAGE INNER  -->
</div>

<%- include('footer.ejs') %>
